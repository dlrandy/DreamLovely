{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-react-hooks"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-first",
        "named-imports-order": "lowercase-first"
      }
    ],
    "react-hooks-nesting": "error"
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
